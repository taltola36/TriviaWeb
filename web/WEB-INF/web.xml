<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Servlets</servlet-name>
        <servlet-class>Servlets</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>myServlets.MyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddQuestion</servlet-name>
        <servlet-class>myServlets.AddQuestion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteQuestion</servlet-name>
        <servlet-class>myServlets.DeleteQuestion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlayGame</servlet-name>
        <servlet-class>myServlets.PlayGame</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>myServlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Welcome</servlet-name>
        <servlet-class>myServlets.Welcome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AboutUs</servlet-name>
        <servlet-class>myServlets.AboutUs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MoreGames</servlet-name>
        <servlet-class>myServlets.MoreGames</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MainMenu</servlet-name>
        <servlet-class>myServlets.MainMenu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveToFile</servlet-name>
        <servlet-class>myServlets.SaveToFile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlets</servlet-name>
        <url-pattern>/Servlets</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/MyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddQuestion</servlet-name>
        <url-pattern>/AddQuestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteQuestion</servlet-name>
        <url-pattern>/DeleteQuestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlayGame</servlet-name>
        <url-pattern>/PlayGame</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Welcome</servlet-name>
        <url-pattern>/Welcome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AboutUs</servlet-name>
        <url-pattern>/AboutUs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MoreGames</servlet-name>
        <url-pattern>/MoreGames</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainMenu</servlet-name>
        <url-pattern>/MainMenu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveToFile</servlet-name>
        <url-pattern>/SaveToFile</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
